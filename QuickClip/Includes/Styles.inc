[StyleContentText]
FontSize = #FontSize#
FontColor = #TextColor#
FontFace = #Font#
FontWeight = #FontWeight#
AntiAlias = 1

[Background]
Meter = Shape
Shape = Rectangle 0,0,#Width#,#Height#,#BackgroundBorderRadius# | Fill Color #BackgroundColor# | StrokeWidth #BackgroundStrokeWidth#
DynamicVariables = 1
